{
  "name": "starker-regen",
  "version": "1.0.0",
  "description": "A simple React template with F#",
  "main": "src/index.fs",
  "scripts": {
    "restore": "dotnet tool restore",
    "dev": "concurrently --kill-others \"pnpm watch:fb\" \"pnpm watch:tw\"",
    "build": "dotnet tool restore && dotnet fable src --run node esbuild.condig.js prod",
    "clean": "dotnet fable clean src --yes",
    "watch:tw": "tailwindcss -i ./src/index.css -o ./public/dist/tailwind.css --watch",
    "watch:fb": "dotnet tool restore && dotnet fable watch src --runFast node esbuild.config.js dev"
  },
  "keywords": [],
  "author": "Shaun",
  "license": "MIT",
  "dependencies": {
    "prism-themes": "^1.9.0",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "chalk": "^4.1.2",
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.10",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7"
  }
}
